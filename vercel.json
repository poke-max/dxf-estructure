{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "dest": "api/upload.py"
    },
    {
      "src": "/api/generate",
      "dest": "api/generate.py"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.py"
    }
  ]
} 